    "testui": "vitest --ui --coverage",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "csv-parse": "^6.1.0",
    "d3-array": "^3.0.2",
    "js-yaml": "^4.1.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "tsup": "^6.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.16"
  }
}
