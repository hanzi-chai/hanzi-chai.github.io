{
  "name": "hanzi-chai",
  "version": "0.3.0",
  "description": "提取自主仓库的汉字拆分/编码核心算法包",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "fetch": "tsx scripts/fetch.ts",
    "build": "tsc -p tsconfig.build.json && cp -r src/data dist/data",
